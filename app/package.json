  "scripts": {
    "start-react": "react-app-rewired start",
    "build-react": "react-app-rewired build",
    "start-electron": "wait-on tcp:3000 && electron-forge start",
    "build-electron": "cp -r ./src/electron/. ./build/electron && mv ./build/electron/env-prod.json ./build/electron/env.json",
    "build": "yarn build-react && yarn build-electron && yarn make",
    "dev": "concurrently -k \"npm:start-react\" \"npm:start-electron\"",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "concurrently": "^5.3.0",
    "electron": "^9.1.2",
    "electron-rebuild": "^1.11.0",
    "react-app-rewired": "^2.1.6",
    "wait-on": "^5.1.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cruster"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
